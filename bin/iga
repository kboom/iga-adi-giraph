#!/usr/bin/env bash
if [[ $# = 0 ]]; then
  echo "Usage: iga [-D<Hadoop property>] <program parameters>"
  exit 1
fi

ENV_DIR="$( cd -P "$( dirname "${BASH_SOURCE:-0}" )" && pwd )"
source ${ENV_DIR}/iga-env
exec "yarn" --config $HADOOP_CONF_DIR jar $JAR $CLASS $HADOOP_PROPERTIES -libjars $GIRAPH_JARS  "$@"
